def date_range(start_date, end_date):
    current_date = datetime.strptime(start_date, "%Y-%m-%d")
    while current_date.strftime("%Y-%m-%d") < end_date:
        print(current_date.strftime("%Y-%m-%d"))
        current_date += timedelta(days=1)


